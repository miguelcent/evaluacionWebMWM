<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
  <navigation-rule>
    <from-view-id>/index.jsf</from-view-id>
    <navigation-case>
      <from-outcome>login</from-outcome>
      <to-view-id>/login.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>register</from-outcome>
      <to-view-id>/register.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/login.jsf</from-view-id>
    <navigation-case>
      <from-outcome>ok</from-outcome>
      <to-view-id>/shop.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>error</from-outcome>
      <to-view-id>/index.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>regiter</from-outcome>
      <to-view-id>/register.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/register.jsf</from-view-id>
    <navigation-case>
      <from-outcome>ok</from-outcome>
      <to-view-id>/shop.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>error</from-outcome>
      <to-view-id>/index.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>login</from-outcome>
      <to-view-id>/login.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/shop.jsf</from-view-id>
    <navigation-case>
      <from-outcome>cart</from-outcome>
      <to-view-id>/shoppingCart.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>details</from-outcome>
      <to-view-id>/userDetail.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>users</from-outcome>
      <to-view-id>/adminUsers.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>sales</from-outcome>
      <to-view-id>/sales.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>products</from-outcome>
      <to-view-id>/products.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>