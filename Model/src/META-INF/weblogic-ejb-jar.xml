    <ejb-name>UserBean</ejb-name>
    <stateless-session-descriptor/>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>UserTypeBean</ejb-name>
    <stateless-session-descriptor/>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>ProductBean</ejb-name>
    <stateless-session-descriptor/>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>ProductCategoryBean</ejb-name>
    <stateless-session-descriptor/>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>SaleBean</ejb-name>
    <stateless-session-descriptor/>
  </weblogic-enterprise-bean>
  <weblogic-enterprise-bean>
    <ejb-name>SaleLineBean</ejb-name>
    <stateless-session-descriptor/>
  </weblogic-enterprise-bean>
</weblogic-ejb-jar>
